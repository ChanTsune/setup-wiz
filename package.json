{
  "name": "setup-wiz",
  "version": "0.0.0",
  "private": true,
  "description": "setup wiz language",
  "author": "ChanTsune",
  "license": "MIT",
  "repository": "https://github.com/ChanTsune/setup-wiz.git",
  "homepage": "https://github.com/ChanTsune/setup-wiz",
  "bugs": "https://github.com/ChanTsune/setup-wiz/issues",
  "keywords": [
    "github",
    "wiz"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -o dist/main src/main.ts && ncc build -o dist/post src/post.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/tool-cache": "^2.0.1",
    "@types/node": "^16.17.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^27.5.1",
    "nock": "13.3.3",
    "prettier": "3.0.3",
    "standard": "^17.1.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">= 16.17.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.[tj]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
